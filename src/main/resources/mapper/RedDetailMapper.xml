<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asule.redpocket.mapper.RedDetailMapper">

    <resultMap id="BaseResultMap" type="com.asule.redpocket.domain.RedDetail">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="recordId" column="record_id" jdbcType="INTEGER"/>
            <result property="amount" column="amount" jdbcType="DECIMAL"/>
            <result property="isActive" column="is_active" jdbcType="TINYINT"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,record_id,amount,
        is_active,create_time
    </sql>

</mapper>
