<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asule.redpocket.mapper.RedRecordMapper">

    <resultMap id="BaseResultMap" type="com.asule.redpocket.domain.RedRecord">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="phone" column="phone" jdbcType="CHAR"/>
            <result property="redPocket" column="red_pocket" jdbcType="CHAR"/>
            <result property="moneyTotal" column="money_total" jdbcType="INTEGER"/>
            <result property="amount" column="amount" jdbcType="INTEGER"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="isActive" column="is_active" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,phone,red_pocket,
        money_total,amount,create_time,
        is_active
    </sql>
</mapper>
